{
    "name": "aman-rawat/elastic-scout-driver-plus",
    "description": "Extension for Elastic Scout Driver",
    "keywords": [
        "scout",
        "elastic",
        "elasticsearch",
        "laravel",
        "driver",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aman Rawat",
            "email": "amanrwt99@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ElasticScoutDriverPlus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElasticScoutDriverPlus\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "aman-rawat/elastic-adapter": "^1.0",
        "aman-rawat/elastic-scout-driver": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.12",
        "laravel/scout": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.32",
        "laravel/legacy-factories": "^1.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElasticScoutDriverPlus\\ServiceProvider"
            ]
        }
    }
}
